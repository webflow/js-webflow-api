{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "camelcase": "off",
    "arrow-body-style": "off",
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "max-classes-per-file": "off",
    "prettier/prettier": ["error"]
  },
  "ignorePatterns": ["node_modules/", "dist/"]
}
